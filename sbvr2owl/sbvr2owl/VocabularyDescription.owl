<rdf:RDF  
xml:base="http://purl.org/sbvr2owl/VocabularyDescription"
     xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
     xmlns:owl="http://www.w3.org/2002/07/owl#"
     xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
     xmlns:xml="http://www.w3.org/XML/1998/namespace"
     xmlns:dct="http://purl.org/dc/terms/"
     xmlns:skos="http://www.w3.org/2004/02/skos/core#"
xmlns:sm="http://www.omg.org/techprocess/ab/20130301/SpecificationMetadata#"
xmlns:sbvr-tt="http://purl.org/sbvr2owl/SBVRTaggedText#"
xmlns:sbvr-vd="http://purl.org/sbvr2owl/VocabularyDescription#">


    <owl:Ontology rdf:about="http://purl.org/sbvr2owl/VocabularyDescription">
        <rdfs:label>SBVR to OWL Vocabulary Description Ontology</rdfs:label>

    <!-- Module-Level Specification Metadata -->

        <sm:moduleName rdf:datatype="&xsd;string">SBVR to OWL Vocabulary Description Ontology</sm:moduleName>
        <sm:moduleVersion rdf:datatype="&xsd;string">1.0</sm:moduleVersion>
        <sm:moduleAbbreviation rdf:datatype="&xsd;string">SBVR-VD</sm:moduleAbbreviation>
        <sm:moduleAbstract rdf:datatype="&xsd;string">This ontology provides a Web Ontology Language (OWL) platform-specific representation of a subset of the concepts and relationships defined in the SBVR 1.1 Specification.  The focus is on core definitions together with the vocabulary description annotations (vocabulary introductory information) given in SBVR Annex C.2.  It is intended for use in transformations from SBVR to OWL, especially where preservation of as much of the original SBVR tagging in definitions is desired.</sm:moduleAbstract>
        <sm:directSource rdf:datatype="&xsd;anyURI">http://www.omg.org/members/cgi-bin/doc?dtc/12-10-10.pdf</sm:directSource>


    <!-- File-Level Specification Metadata -->

        <sm:filename rdf:datatype="&xsd;string">VocabularyDescription.owl</sm:filename>
        <sm:contentLanguage rdf:datatype="&xsd;anyURI">http://www.w3.org/standards/techs/owl#w3c_all</sm:contentLanguage>


